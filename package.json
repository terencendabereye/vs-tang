{
    "name": "tang-language-support",
    "displayName": "Tang Language Support",
    "description": "Syntax Highlighting for tang language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "tang",
            "aliases": ["Tang", "tang"],
            "extensions": [".tang"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "tang",
            "scopeName": "source.tang",
            "path": "./syntaxes/tang.tmLanguage.json"
        }]
    }
}